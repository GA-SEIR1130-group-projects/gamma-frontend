> {\n                        return(\n                            <div \n                                class=\"user-comment\" \n                                onClick={() => deleteComment(comment._id)}\n                            >\n                                <p>user: {comment.comment}</p>\n                                <button\n                                    type=\"button\" class=\"btn btn-danger\"\n                                    onClick={() => deleteComment(comment._id)}\n                                >Delete</button>\n                                <button \n                                    type=\"button\" class=\"btn btn-info\"\n                                    onClick={() => updateComment(comment._id, \"updated comment\")}\n                                >Update</button>\n                            </div>\n                        );\n                    })\n                }\n            </div>\n\n            <form className=\"add-comment\" onSubmit={handleSubmit}>\n                <label>Comment:</label>\n                <input\n                    placeholder=\"Add a comment...\" \n                    type=\"text\"\n                    value={newComment}\n                    onChange={handleChange}\n                />\n                <button type=\"submit\">post</button>\n            </form>\n        </div>\n    );\n}\n\nexport default PicturePost;\n\n\n","/Users/biprajit163/Desktop/SEI/projects/project3/gamma-frontend/src/reportWebVitals.js",[],"/Users/biprajit163/Desktop/SEI/projects/project3/gamma-frontend/src/Components/login/Log.jsx",["65","66","67","68","69","70"],"/Users/biprajit163/Desktop/SEI/projects/project3/gamma-frontend/src/Components/UserProfile-Components/EditProfile.jsx",[],{"ruleId":"71","severity":1,"message":"72","line":6,"column":8,"nodeType":"73","messageId":"74","endLine":6,"endColumn":13},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"75","replacedBy":"79"},{"ruleId":"77"